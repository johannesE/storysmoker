<%- model_class = Story -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>

<div class="handWritten">  
<ul>

  <% @stories.where(status: 'finished').each do |story| %>
		<li><%= link_to story.title, story_path(story) %></li>



  <% end %>	
</ul>
</div>



			
			
			
